<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <!-- jdbc propertie file -->
  <properties resource="classpath:jdbc.properties"/>
  
  <!--可选属性 defaultModelType="conditional" -->
  <!-- 不生产example：targetRuntime="MyBatis3Simple" -->
  <context id="context1" defaultModelType="flat" targetRuntime="MyBatis3Simple">
  	
  	<!-- 自定义注释：type="me.skyline.common.mybatis.MyCommentGenerator" -->
  	<commentGenerator>
  			 <!-- 去除自动生成的时间戳 true：是 ： false:否 -->
             <property name="suppressDate" value="true"/>
             <!-- 去除自动生成的注释 true：是 ： false:否 -->
             <property name="suppressAllComments" value="true"/>
  	</commentGenerator>
  	
  	<!-- 数据库连接信息 -->
    <jdbcConnection driverClass="${driver}" 
    				connectionURL="${url}" 
   				 	userId="${user}" 
   				 	password="${password}" />
   
    <!-- 实体类 -->
    <javaModelGenerator targetPackage="com.innofarms.web.filter" targetProject="innofarm" >
    	<property name="constructorBased" value="false"/>
    	<!-- 对数据库查询结果进行trim操作 -->
    	<property name="trimStrings" value="true"/>
    </javaModelGenerator>
    
    <!-- sql-->
    <sqlMapGenerator targetPackage="com.innofarms.web.filter" targetProject="innofarm" >
    	<property name="enableSubPackages" value="true"/>
    	
    </sqlMapGenerator>
    
    <!-- mapper接口 -->
    <javaClientGenerator targetPackage="com.innofarms.web.filter" targetProject="innofarm" type="XMLMAPPER" >
    	<property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    
    <!-- 字段映射 -->
    <!-- tableName="%" :生成全部的表 -->
    <table tableName="user_info" domainObjectName="UserInfo" enableCountByExample="false" 
    		enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
    		selectByExampleQueryId="false">
      <columnOverride column="USER_ACNT" property="phoneNum" />
    </table>
    
  </context>
</generatorConfiguration>